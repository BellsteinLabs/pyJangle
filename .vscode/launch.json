    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "pyjangle.command.register_test",
            "justMyCode": true
        },
        {
            "name": "Dart: Attach to Process",
            "type": "dart",
            "request": "attach"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "./"
            }
        }
    ]
}